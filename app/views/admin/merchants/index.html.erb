

<h1>Admin Merchant Index Page</h1>

<h2>Merchants</h2>

<% @merchants.each do |merchant| %>
  <section id= <%= "merchant-#{merchant.id}" %>>
    <h3><%= merchant.name %></h3>
    <p><%= "#{merchant.city}, #{merchant.state}" %></p>

    <p><%= button_to "Disable" %></p>
    <p><%= button_to "Enable" %></p>
  </section>
<% end %>
